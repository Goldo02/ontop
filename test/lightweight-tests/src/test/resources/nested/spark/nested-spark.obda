[PrefixDeclaration]
:		http://person.example.org/
p:		http://pub.example.org/
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
obda:		https://w3id.org/obda/vocabulary#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[

mappingId	MAPID-person-tags
target		:person/{id} a :Person ; :fullName {fullname} ; :ssn {ssn}^^xsd:int ; :tag_str {tag_str} ; :tag_int {tag_int}^^xsd:int ; :tag_ids {tag_ids} .
source		SELECT id, fullname, ssn, tag_str, tag_int, CAST(tags as STRING) AS tag_ids FROM hr.`person-tags`;

mappingId	MAPID-person-friend
target		:person/{id} a :Person ; :hasFriend :person/{id}/{pos} . :person/{id}/{pos} a :Friend ; :firstName {firstName} ; :nickName {nickName_str} ; :city {city} .
source  	SELECT id, pos, firstName, nickName_str, city FROM hr.`persons-friends`;
]]

[MappingDeclaration] @collection [[
mappingId	MAPID-pub-person
target		p:person/{id} a p:Person ; p:name {name}.
source		SELECT id, name FROM person;
]]

[MappingDeclaration] @collection [[
mappingId	MAPID-pub
target		p:publication/{id_pub} a p:Publication ; p:title {title} ; p:year {year} ; p:venue {venue}.
source		SELECT id_pub, title, year, venue FROM pub.publication;
]]

[MappingDeclaration] @collection [[
mappingId	MAPID-author
target		p:person/{id} p:author p:publication/{id_pub}.
source		SELECT id, id_pub FROM pub.publication;
]]